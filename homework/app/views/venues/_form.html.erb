<% if @venue.errors.any? %>


<ul>
      <% @venue.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<%= form_for (@venue) do |f| %>
	
<div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>

  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>